{
  "name": "Shareable Color Palette",
  "description": "Creates a new artboard with all the colors and HEX values for 'Document Colors' palette.",
  "author": "Lalo Mrtnz & Adrxx",
  "homepage": "https://github.com/LaloMrtnz/ShareableColorPalette",
  "version": "2.0",
  "identifier": "com.lalomrtnz.sketch.shareable-color-palette",
  "compatibleVersion": "3.5.2",
  "bundleVersion": "1.0",

  "commands" : [
    {
      "script" : "script.js",
      "handler" : "createColorPalette",
      "shortcut" : "cmd ctrl c",
      "name" : "Create Shareable Color Palette",
      "identifier" : "shareablecolorpalette"
    },
    {
      "name": "Generate SASS Color Palette",
      "identifier": "generateSASS",
      "shortcut": "",
      "script": "sass_generator.js",
      "handler": "generateSASS"
    }
  ],
  "menu" : {
    "items" : [
      "shareablecolorpalette",
      "generateSASS"
    ],
    "isRoot": true,
    "title" : "Shareable Color Palette"
  },

}